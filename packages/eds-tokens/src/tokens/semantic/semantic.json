{
  "eds": {
    "semantic": {
      "cta": {
        "label": {
          "value": {
            "fontSizes": "{fontSize.2}",
            "lineHeights": "{lineHeight.tight.2}",
            "verticalPadding": "({eds.core.sizing.icon.default} - {lineHeight.tight.2}) / 2",
            "horizontalPadding": "{spacing.4}",
            "itemSpacing": "{spacing.0}"
          },
          "type": "composition"
        },
        "container": {
          "medium": {
            "value": {
              "horizontalPadding": "{spacing.12}",
              "verticalPadding": "({eds.core.sizing.cta.height.medium} - ({eds.semantic.cta.label.lineHeights} + {eds.semantic.cta.label.verticalPadding} * 2)) / 2"
            },
            "type": "composition"
          }
        },
        "color": {
          "background": {
            "primary": {
              "enabled": {
                "value": "orange",
                "type": "color"
              }
            }
          }
        }
      }
    }
  }
}